{
"manifest_version": 3,
"name": "Midi Player Extension",
"version": "1.0",
"description": "A simple MIDI file player as a Chrome extension.",
"permissions": [
"storage"
],
"action": {
"default_popup": "index.html",
"default_icon": "icon.png"
},
"icons": {
"16": "icon16.png",
"48": "icon48.png",
"128": "icon128.png"
}
}